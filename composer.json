{
  "name": "data-dog/acl-bundle",
  "type": "bundle",
  "description": "ACL management bundle",
  "keywords": [
    "acl", "permission", "access control lists", "security"
  ],
  "license": "MIT",

  "autoload": {
    "psr-0": {
      "AclBundle\\": "src/"
    }
  },

  "require": {
    "php": ">=5.4.0",
    "symfony/symfony": "~2.5"
  },

  "require-dev": {
    "phpunit/phpunit": "~4.4",
    "phpspec/prophecy-phpunit": "~1.0"
  },

  "archive": {
    "exclude": ["vendor", "tests", "bin"]
  },

  "config": {
    "bin-dir": "bin"
  },

  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
